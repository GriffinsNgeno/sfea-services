# Имя Репозитория (Только для helm repo add) 
REPONAME=cetic
# URL  Репозитория (Только для helm repo add)
REPOURL=https://cetic.github.io/helm-charts

# Имя chart'а для установки(для helm upgrade: Для локального установить путь к Chart.yaml: "CHARTNAME=RELEASENAME/" или если Chart.yaml в корне: "CHARTNAME=.")
CHARTNAME=cetic/adminer

# Имя release. (Для helm upgrade/helm install/helm create: в примерах для helm обычно my-release)
RELEASENAME=adminer

# Namespace для установки/удаления
NS=default

#Дополнительные параметры для каждой команды.
ADD_INSTALL=
ADD_UNINSTALL=
 