{
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "App\\": "src/",
      "Tests\\": "tests/"
    },
    "files": [
      "src/functions/log_functions.php",
      "src/functions/resource_functions.php"
    ]
  },
  "require": {
    "aws/aws-sdk-php": "^3.306",
    "ext-json": "*",
    "ext-mysqli": "*",
    "monolog/monolog": "2.x-dev",
    "php": "7.2.24",
    "sentry/sdk": "dev-master",
    "symfony/var-dumper": "5.4.x-dev",
    "webonyx/graphql-php": "14.x-dev",
    "nilportugues/sql-query-builder": "^1.8",
    "jonahgeorge/jaeger-client-php": "dev-master"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true,
      "symfony/runtime": true
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "require-dev": {
    "phpunit/phpunit": "8.5.x-dev"
  }
}
