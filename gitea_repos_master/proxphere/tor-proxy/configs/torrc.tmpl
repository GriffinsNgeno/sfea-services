ClientOnly 1
ClientUseIPv6 0
ConnectionPadding 0
FastFirstHopPK 1
HardwareAccel 1
StrictNodes 1
UseEntryGuards 1
MaxMemInQueues {{ .MaxMemInQueuesMB }}MB
{{- $entryNodes := .EntryNodes.List }}
