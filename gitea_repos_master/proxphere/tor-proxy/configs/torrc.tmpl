EntryNodes {{ range $k, $v := $entryNodes }}{{ if gt $k 0 }},{{ end }}{{ `{` }}{{ $v }}{{ `}` }}{{ end }}
{{- end }}
{{- $exitNodes := .ExitNodes.List }}
{{- if gt ($exitNodes | len) 0 }}
ExitNodes {{ range $k, $v := $exitNodes }}{{ if gt $k 0 }},{{ end }}{{ `{` }}{{ $v }}{{ `}` }}{{ end }}
{{- end }}
{{- $excludeNodes := .ExcludeNodes.List }}
{{- if gt ($excludeNodes | len) 0 }}
ExcludeNodes {{ range $k, $v := $excludeNodes }}{{ if gt $k 0 }},{{ end }}{{ `{` }}{{ $v }}{{ `}` }}{{ end }}
{{- end }}
