{
  "name": "autoins-web-api",
  "version": "1.0.0",
  "main": "index.ts",
  "author": "kovinevmv <kovinevmv@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "prepare:proxy": "ts-node ./src/call/proxy.ts",
    "slimer:dev": " SLIMERJSLAUNCHER=./firefox/firefox ./node_modules/.bin/slimerjs --config $INIT_CWD/config.json --profile ./firefox/profile ./_bundles/1.js",
    "slimer:prod": "SLIMERJSLAUNCHER=./firefox/firefox ./node_modules/.bin/slimerjs --config $INIT_CWD/config.json --profile ./firefox/profile --headless ./_bundles/dist.js",
    "start:dev": "yarn build; yarn slimer:dev",
    "start:prod": "yarn prepare:proxy; PROD=true yarn slimer:prod",
    "lint": "eslint src --fix",
    "prettier": "prettier --write --loglevel=silent 'src/**/*.ts'",
    "refactor": "yarn run prettier && yarn run lint"
  },
  "dependencies": {
    "@types/node": "^17.0.42",
    "dotenv": "^16.0.1",
    "js-logger": "^1.6.1",
    "redis": "^4.2.0",
    "slimerjs": "^1.0.0",
    "typescript": "^4.7.3"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.18.9",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@types/eslint": "^6.1.1",
    "@types/node-ipc": "^9.2.0",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "babel": "^6.23.0",
    "babel-loader": "^8.2.5",
    "dotenv-webpack": "^8.0.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.6.2",
    "terser-webpack-plugin": "^5.3.3",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.8.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-node-externals": "^3.0.0"
  }
}
